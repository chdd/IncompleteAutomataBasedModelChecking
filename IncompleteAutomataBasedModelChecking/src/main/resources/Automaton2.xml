                	<character>b</character>
                	<destinationState>s5</destinationState>
                </transition> 
                <transition>
                	<character>c</character>
                	<destinationState>s6</destinationState>
                </transition> 
            </transitions>
        </element>
         <element>
            <sourceState>s6</sourceState>
            <transitions>
            	 <transition>
                	<character>a</character>
                	<destinationState>s6</destinationState>
                </transition>
            	 <transition>
                	<character>b</character>
                	<destinationState>s6</destinationState>
                </transition> 
                <transition>
                	<character>c</character>
                	<destinationState>s6</destinationState>
                </transition> 
            </transitions>
        </element>
    </transitionRelation>
    <alphabet>
        <character>a</character>
        <character>b</character>
        <character>c</character>
    </alphabet>
      <transparentStates>
    </transparentStates>
