<buchiAutomaton>
    <initialStates>
        <initialState>s4</initialState>
    </initialStates>
    <states>
        <state id="s4"/>
        <state id="s5"/>
        <state id="s6"/>
    </states>
    <acceptingStates>
    	<acceptingState>s6</acceptingState>
    </acceptingStates>
   
    <transitionRelation>
        <element>
            <sourceState>s4</sourceState>
